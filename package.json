{
  "name": "admiral-cli",
  "version": "0.11.1",
  "description": "A Command Line Framework (CLI) framework for Node.js. Admiral has features like other CLI frameworks, but adds validation and some callbacks in key places. Less configuration, stronger validation.",
  "main": "main",
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/four43/admiral-cli.git"
  },
  "keywords": [
    "cli",
    "framework",
    "parameters",
    "parsing",
    "commands"
  ],
  "author": "Seth Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/four43/admiral-cli/issues"
  },
  "dependencies": {
    "columnify": "^1.2.1",
    "debug": "^2.1.3"
  },
  "devDependencies": {
    "assert": ">=1.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0"
  }
}
